      <trkpt lat="-27.1350" lon="-48.4720">
        <name>Dia 12 - Praia da Sepultura (Bombinhas)</name>
        <desc>Aguas cristalinas! Snorkel incrivel. TPA R$ 3,50 moto.</desc>
      </trkpt>
      <trkpt lat="-27.8900" lon="-48.5800">
        <name>Dia 12 - Guarda do Embau</name>
        <desc>9a Reserva Mundial de Surfe! Primeira do Brasil. Travessia de canoa R$ 5-10.</desc>
      </trkpt>

