      <trkpt lat="-22.9797" lon="-49.8708">
        <name>Dia 15 - Ourinhos - SP (Pernoite)</name>
        <desc>400 km via BR-376/BR-153.</desc>
      </trkpt>

      <!-- Dia 16: Ourinhos → Uberaba (360 km) -->
      <trkpt lat="-19.7500" lon="-47.9318">
        <name>Dia 16 - Uberaba - MG (Pernoite)</name>
        <desc>360 km.</desc>
      </trkpt>

      <!-- Dia 17: Uberaba → Goiania (380 km) -->
      <trkpt lat="-16.6869" lon="-49.2648">
        <name>Dia 17 - Goiania - GO (CHEGADA)</name>
        <desc>380 km. FIM DA VIAGEM! Chegada ~12:00.</desc>
      </trkpt>

