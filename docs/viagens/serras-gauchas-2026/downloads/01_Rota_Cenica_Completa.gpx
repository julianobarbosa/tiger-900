      <trkpt lat="-25.4355" lon="-49.2748">
        <name>Dia 14 - Serra Verde Express (Embarque)</name>
        <desc>Estacao Rodoferroviaria 08:30. RESERVAR ANTECIPADO! Lado DIREITO melhor vista.</desc>
      </trkpt>
      <trkpt lat="-25.4800" lon="-48.8400">
        <name>Morretes - PR</name>
        <desc>Chegada 12:45. BARREADO obrigatorio! Retorno van ~R$ 50.</desc>
      </trkpt>
      <trkpt lat="-25.3950" lon="-49.3350">
        <name>Santa Felicidade</name>
        <desc>Jantar italiano! Bairro italiano de Curitiba.</desc>
      </trkpt>

